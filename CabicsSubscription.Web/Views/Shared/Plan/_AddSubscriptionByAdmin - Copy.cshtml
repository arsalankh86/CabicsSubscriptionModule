
@{
    ViewBag.Title = "_AddSubscriptionByAdmin";
}

<!-- Bootstrape CSS and JS -->
<link href="/assets/bootstrap/css/bootstrap.min.css" rel="stylesheet" type="text/css">
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
<script src="/assets/bootstrap/js/bootstrap.min.js"></script>

<!-- Font Awesome CSS -->
<link href="assets/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">

<!-- VS CSS -->
<link href="/css/subscription-style.css" rel="stylesheet" type="text/css">
<link href="/css/subscription-responsive.css" rel="stylesheet" type="text/css">





<div class="site-width" id="payment-tabs">
    <div id="alerts"></div>
    <h1 id="plan-name-title">
        <span id="dvbuycredit" name="dvbuycredit">   </span>
        <div id="dvmonthly" name="dvmonthly"></div>
    </h1>

    <section>
        <input type="hidden" name="plantype" id="plantype" />
        <div class="form-group">
            <label for="fname">Cab Office:</label>
            @*<select id="ddlcaboofice" class="myDropDownLisTId"></select>*@

        </div>
        <div class="form-group">
            <label for="lname">Cheque no:</label>
            <input type="text" class="form-control" id="chqueno">
        </div>

        <div id="dvpayasyougo" style="display:none">
            <div class="form-group">
                <label>Credit: (£<span id="lblprice" name="lblprice"></span>)</label>
                <input type="text" class="form-control" id="qty" name="qty">
            </div>
        </div>

        <div class="form-group">
            Total: <input type="text" class="form-control" id="totalamount" name="totalamount" disabled="disabled">
        </div>

        <div class="bt-drop-in-wrapper">
            <div id="bt-dropin"></div>
        </div>
    </section>
    <button class="button" type="button" id="btnsubmut" name="btnsubmut"><span>Purchase For CabOffice</span></button>

</div>


<h1 class="text-center">Add Subscription By Admin</h1>

<br><br>

<div class="my-body">
    <div class="row">
        <div class="col-md-6">
            <div class="my-field">
                <ul>
                    <li>
                        <label>Cab Office</label>
                        <select id="ddlcaboofice" ></select>
                    </li>
                    <li>
                        <label>&nbsp;</label>
                        <input type="text" placeholder="">
                    </li>
                </ul>
            </div>
            <div class="my-field">
                <label>Cheque no</label>
                <input type="text" class="form-control" id="chqueno">
            </div>
        </div>
        <div class="col-md-6">
            <div class="my-field">
                <label>Credit: (£50)</label>
                <input type="text" placeholder="">
            </div>
            <div class="my-field">
                <label>Total</label>
                <input type="text" placeholder="" disabled value="100">
            </div>
        </div>
    </div>
</div>
<div class="my-footer">
    <button class="btn btn-success btn-lg">Purchase for Cab Office</button>
</div>



